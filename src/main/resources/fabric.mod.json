{
  "schemaVersion": 1,
  "id": "como-client",
  "version": "${version}",

  "name": "Como Client",
  "description": "A Minecraft utility mod developed with the aim of giving its users an advantage in most (if not all) situations (but since I made it it probably won't to be honest...), while still aiming to be as simplistic as possible.",
  "authors": [
    "gingerchicken"
  ],
  "contact": {
    "homepage": "https://example.com/pls-no-bully-i-dont-own-a-domain",
    "sources": "https://github.com/gingerchicken/como-client"
  },

  "license": "GNU-3.0",
  "icon": "assets/como-client/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.como.client.Main"
    ]
  },
  "mixins": [
    "como-client.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
